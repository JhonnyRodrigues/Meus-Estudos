algoritmo "Contador500"
// Função : Contando a Taboada do 10 até 500
// Autor : Jonatas
// Data : 03/05/2019
var
   contador : inteiro
inicio
	contador <- 0
	Enquanto (CONTADOR <= 500) faca
		EscrevaL(contador)
		contador <- contador + 10
	FimEnquanto
	EscrevaL("Terminei de contar")
fimalgoritmo
