algoritmo "Contagem_Regressiva"
// Função : contagem regressiva a partir de 10 até 0.
// Autor : Jonatas
// Data : 03/05/2019
var
   contador : inteiro
inicio
contador <- 10
	Enquanto (CONTADOR >= 0) faca
		EscrevaL(contador)
		contador <- contador - 1
	FimEnquanto
	EscrevaL("Terminei de contar")
fimalgoritmo
FimSe
